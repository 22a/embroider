{
  "name": "@embroider/sample-v2-addon",
  "version": "0.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "scripts": {
    "test:classic": "cd test-app && EMBROIDER_TEST_SETUP_FORCE=classic ember test",
    "test:embroider": "cd test-app && EMBROIDER_TEST_SETUP_FORCE=embroider ember test",
    "start": "cd test-app && EMBROIDER_REBUILD_ADDONS=@embroider/sample-v2-addon ember s"
  },
  "dependencies": {
    "@embroider/util": "0.35.0"
  },
  "devDependencies": {},
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": "app"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
